  'sender' => $connectiouserUidDetails[$requesterUser]->mail,
  'body' => $body,
  'language' => language_default(),
  'key' => 'connection_request_connect',
  'module' => 'm6connect_misc',
  );
  $mail = drupal_mail ($params['module'], $params['key'], $to,$params['language'], $params );
  return $mail;
}

