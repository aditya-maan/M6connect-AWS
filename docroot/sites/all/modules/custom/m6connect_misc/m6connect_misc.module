            $replacements['[node:customDocumentType]'] = check_plain($documentsFolderNames[$data['node']->type]);

            //print "<pre>"; print_r($replacements['[node:customDocumentType]']); die;
        }
// Set Company Title
        /* $cmpNid ='';
          $cmpNode='';
