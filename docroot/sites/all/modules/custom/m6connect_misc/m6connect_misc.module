function m6connect_misc_download_attachment($fid) {
    $file = file_load($fid);
    $headers = file_get_content_headers($file);
    $headers['Content-Disposition'] = 'attachment; filename="' . $file->filename . '"';
    $headers['width'] = '50px';
    $headers['height'] = '50px';
    file_transfer($file->uri, $headers);
}

