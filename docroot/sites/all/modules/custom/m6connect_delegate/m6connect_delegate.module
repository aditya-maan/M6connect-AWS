function add_your_skill_custom_form($form, &$form_state){
   global $user;
  
   $form['skill_uid']= array(
    '#type' => 'value',
	'#value' => $user->uid,
   );
   $form['#attached']['css'][] = drupal_get_path('module', 'halfstar') . '/css/halfstar.css';
