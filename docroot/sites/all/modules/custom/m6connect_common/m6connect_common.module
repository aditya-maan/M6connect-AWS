  } else if ($action == 'disagree' && isset ( $_REQUEST ['award_data'] )) {
    $awardData = $_REQUEST ['award_data'];
    $explode_data = explode ( "@@", $awardData );
    $award_taker = $explode_data [0];
    $award_page = $explode_data [1];
    $award_giver = $explode_data [2];
